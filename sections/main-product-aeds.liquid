{% comment %}theme-check-disable TemplateLength{% endcomment %}
{{ 'section-main-product.css' | asset_url | stylesheet_tag }}
{{ 'component-price.css' | asset_url | stylesheet_tag }}
{{ 'component-facets.css' | asset_url | stylesheet_tag }}
{{ 'component-rating.css' | asset_url | stylesheet_tag }}
{{ 'component-pills.css' | asset_url | stylesheet_tag }}
{{ 'component-tooltip.css' | asset_url | stylesheet_tag }}

<link rel="stylesheet" href="{{ 'component-reviews.css' | asset_url }}" media="print" onload="this.media='all'">
<link rel="stylesheet" href="{{ 'component-deferred-media.css' | asset_url }}" media="print" onload="this.media='all'">

<noscript>{{ 'component-reviews.css' | asset_url | stylesheet_tag }}</noscript>

{% liquid
	assign first_3d_model = product.media | where: "media_type", "model" | first
	assign product_form_id = 'product-form-' | append: section.id
	assign variant_images = product.images | where: 'attached_to_variant?', true | map: 'src'

	assign is_pre_order = false
	if product.template_suffix == 'pre-order'
		assign is_pre_order = true
	endif
%}

{%- if first_3d_model -%}
	<link id="ModelViewerStyle" rel="stylesheet" href="https://cdn.shopify.com/shopifycloud/model-viewer-ui/assets/v1.0/model-viewer-ui.css" media="print" onload="this.media='all'">
	<link id="ModelViewerOverride" rel="stylesheet" href="{{ 'component-model-viewer-ui.css' | asset_url }}" media="print" onload="this.media='all'">
{%- endif -%}

<div class="container {% if product.media.size == 0 %}container-narrow{% endif %}">
	<div class="block-section">
		<div class="product product-page {% if product.media.size == 0 %}product-no-media{% endif %}">
			{% if product.media.size > 0 %}
				<div class="product-media-gallery-wrap">
					<a class="skip-link visually-hidden visually-hidden-focusable" href="#Product-{{ section.id }}">{{ 'accessibility.link_messages.skip_to_product_info' | t }}</a>
					<media-gallery id="MediaGallery-{{ section.id }}" role="region" class="product-media-gallery" aria-label="{{ 'products.product.media.gallery_viewer' | t }}" hide-variants="{{ section.settings.hide_variants }}">
						<div class="product-media-wrapper">
							{% if product.media %}
								{%- assign featured_media = product.selected_or_first_available_variant.featured_media | default: product.featured_media -%}

								<div class="product-media-main-wrapper">
									{%- assign media_position = 1 -%}
									<div class="product-media-main product-media is-active" data-media-id="{{ featured_media.id }}">
										{% render 'product-gallery-media',
											media: featured_media,
											position: media_position,
											media_width: 800,
											lazyload: false,
											modal_id: section.id,
											image_zoom_type: section.settings.image_zoom_type
										%}
									</div>

									{%- for media in product.media -%}
										{%- if media.id != featured_media.id -%}
											{%- assign media_position = media_position | default: 1 | plus: 1 -%}
											<div class="product-media-main product-media" data-media-id="{{ media.id }}">
												{% render 'product-gallery-media',
													media: media,
													position: media_position,
													media_width: 800,
													lazyload: true,
													modal_id: section.id,
													image_zoom_type: section.settings.image_zoom_type
												%}
											</div>
										{%- endif -%}
									{%- endfor -%}
								</div>

								{%- assign media_position = 1 -%}
								<div class="product-media-scroll-wrap">
									<div class="product-media-scroller">
										<div class="product-media-thumbnails">
											<div class="product-media-thumbnail product-media is-active
													{% if section.settings.hide_variants and variant_images contains featured_media.src %} product-media-thumbnail-variant{% endif %}"
												data-media-id="{{ featured_media.id }}">
												{% render 'product-gallery-media-thumbnail',
													media: featured_media, position: media_position, media_width: 400, lazyload: false
												%}
											</div>

											{% for media in product.media %}
												{%- if media.id != featured_media.id -%}
													{%- assign media_position = media_position | default: 1 | plus: 1 -%}
													<div class="product-media-thumbnail product-media
															{% if section.settings.hide_variants and variant_images contains media.src %} product-media-thumbnail-variant{% endif %}"
														data-media-id="{{ media.id }}">
														{% render 'product-gallery-media-thumbnail',
															media: media, position: media_position, media_width: 400, lazyload: true
														%}
													</div>
												{% endif %}
											{% endfor %}
										</div>
									</div>

									{% if section.settings.thumbnail_navigation %}
										<product-thumbnail-navigation for="MediaGallery-{{ section.id }}" class="hidden">
											<div class="pill-nav pill-nav-slider media-pill-nav-slider">
												<button class="pill-nav-item pill-nav-item-prev pill-nav-item-disabled focus-none js-prev color-background-1" aria-disabled="true">
													<span class="visually-hidden">{{ 'sections.slideshow.previous_slide' | t }}</span>
													{% render 'angle', direction: 'up' %}
												</button>
												<button class="pill-nav-item pill-nav-item-next js-next color-background-1" aria-disabled="false">
													<span class="visually-hidden">{{ 'sections.slideshow.next_slide' | t }}</span>
													{% render 'angle', direction: 'down' %}
												</button>
											</div>
										</product-thumbnail-navigation>
									{% endif %}
								</div>
							{% endif %}
						</div>
					</media-gallery>

					{% if section.settings.image_zoom_type != 'none' %}
						<product-modal id="ProductModal-{{ section.id }}" class="product-media-modal media-modal">
							<div
								class="product-media-modal-dialog"
								role="dialog"
								aria-modal="true"
								aria-label="{{ 'products.product.media.gallery' | t }}"
								tabindex="-1"
							>
								<button id="ModalClose-{{ section.id }}" type="button" class="product-media-modal-toggle" aria-label="{{ 'accessibility.close' | t }}">
									{% render 'icon-cross' %}
								</button>
								<div class="product-media-modal-content" role="document" aria-label="{{ 'products.product.media.gallery' | t }}" tabindex="0">
									{%- liquid
										if product.selected_or_first_available_variant.featured_media != null
											assign media = product.selected_or_first_available_variant.featured_media
											render 'product-gallery-media-modal', media: media, variant_image: section.settings.hide_variants
										endif
									-%}

									{%- for media in product.media -%}
										{%- liquid
											assign variant_image = false
											if section.settings.hide_variants and variant_images contains media.src
												assign variant_image = true
											endif

											unless media.id == product.selected_or_first_available_variant.featured_media.id
												render 'product-gallery-media-modal', media: media, variant_image: variant_image
											endunless
										-%}
									{%- endfor -%}
								</div>
							</div>
						</product-modal>
					{% endif %}

					{% if section.settings.image_disclaimer != blank %}
						<div class="product-media-gallery-message">
							<div class="rte">
								{{ section.settings.image_disclaimer }}
							</div>
						</div>
					{% endif %}
				</div>
			{% endif %}


          <div class="product-desc-data">
              <div class="product-desc-main">
                  {% if product.selected_or_first_available_variant.sku != blank %}
                    <span class="product-info-meta-item product-info-meta-item-code">
                        {{ 'products.product.sku' | t }}: <span id="ProductSku-{{ section.id }}">{{ product.selected_or_first_available_variant.sku }}</span>
                    </span>
                  {% endif %}
                 <div class="product-title product-info-heading h2">{{ product.title }}</div>
                 <section class="pp-desc">
                 <div class="product-desc">{{ product.description }}</div>
                 </section>
              </div>
          </div>


			<div class="product-info-wrap-content">
				<div id="Product-{{ section.id }}" class="product-info">
					{% for block in section.blocks %}
						{% case block.type %}
							{% when '@app' %}
								<div class="product-info-secondary" {{ block.shopify_attributes }}>
									{% render block %}
								</div>

							{%- when 'text' -%}
								{% if block.settings.text != blank or block.settings.text_2 != blank or block.settings.text_3 != blank or block.settings.text_4 != blank %}
									{% if block.settings.color_scheme != "none" %}
										<div class="product-info-secondary card color-{{ block.settings.color_scheme }}" {{ block.shopify_attributes }}>
									{% else %}
										<div class="product-info-secondary" {{ block.shopify_attributes }}>
									{% endif %}
										{% if block.settings.text != blank %}
											<div class="product-info-secondary-line">
												{% if block.settings.icon != blank %}
													{% render 'icons', icon: block.settings.icon %}
												{% endif %}
												{{ block.settings.text }}
											</div>
										{% endif %}
										{% if block.settings.text_2 != blank %}
											<div class="product-info-secondary-line">
												{% if block.settings.icon_2 != blank %}
													{% render 'icons', icon: block.settings.icon_2 %}
												{% endif %}
												{{ block.settings.text_2 }}
											</div>
										{% endif %}
										{% if block.settings.text_3 != blank %}
											<div class="product-info-secondary-line">
												{% if block.settings.icon_3 != blank %}
													{% render 'icons', icon: block.settings.icon_3 %}
												{% endif %}
												{{ block.settings.text_3 }}
											</div>
										{% endif %}
										{% if block.settings.text_4 != blank %}
											<div class="product-info-secondary-line">
												{% if block.settings.icon_4 != blank %}
													{% render 'icons', icon: block.settings.icon_4 %}
												{% endif %}
												{{ block.settings.text_4 }}
											</div>
										{% endif %}
									</div>
								{% endif %}
                            {% when 'sku' %}
                               {% if product.selected_or_first_available_variant.sku != blank %}
                                    <span class="product-info-meta-item product-info-meta-item-code">
                                        {{ 'products.product.sku' | t }}: <span id="ProductSku-{{ section.id }}">{{ product.selected_or_first_available_variant.sku }}</span>
                                    </span>
  							{% endif %}
                          
							{% when 'title' %}
								<div class="product-info-main" {{ block.shopify_attributes }}>
									{% if block.settings.show_custom_badges %}
										{% render 'card-product-custom-badges', product: product, class: 'product-info-badges' %}
									{% endif %}

									{% if block.settings.show_vendor and product.vendor != blank %}
										{% liquid
											assign matching_collection = false
											assign vendor_handle = product.vendor | handleize
											for collection in product.collections
												if collection.handle == vendor_handle
													assign matching_collection = true
												endif
											endfor
										%}
										<div class="product-info-eyebrow">
											{% if matching_collection %}
												<a href="{{ routes.collections_url }}/{{ vendor_handle }}">{{ product.vendor }}</a>
											{% else %}
												{{ product.vendor | link_to_vendor }}
											{% endif %}
										</div>
									{% endif %}

									<h1 class="product-info-heading h2">
										{{ product.title | escape }}
									</h1>

									{% liquid
										assign product_info_meta_stretch_class = ''
										if product.selected_or_first_available_variant.barcode and block.settings.show_barcode and product.selected_or_first_available_variant.sku and block.settings.show_sku
											assign product_info_meta_stretch_class = 'product-info-meta-stretch'
										endif
									%}

									{% if block.settings.show_stock or block.settings.show_sku or block.settings.show_barcode or block.settings.show_rating %}
										<div class="product-info-meta {{ product_info_meta_stretch_class }}">
											{% unless settings.catalog_enabled %}
												{% if block.settings.show_stock %}
													<div id="ProductAvailability-{{ section.id }}" class="product-info-meta-item">
														{% render 'product-availability', product: product %}
													</div>
												{% endif %}
											{% endunless %}

											{% if block.settings.show_sku %}
												{% if product.selected_or_first_available_variant.sku != blank %}
													<span class="product-info-meta-item product-info-meta-item-code">
														{{ 'products.product.sku' | t }}: <span id="ProductSku-{{ section.id }}">{{ product.selected_or_first_available_variant.sku }}</span>
													</span>
												{% endif %}
											{% endif %}

											{% if block.settings.show_barcode %}
												{% if product.selected_or_first_available_variant.barcode != blank %}
													<span class="product-info-meta-item product-info-meta-item-code">
														{% if block.settings.barcode_label != blank %}
															{{ block.settings.barcode_label }}:
														{% endif %}
														<span id="ProductBarcode-{{ section.id }}">{{ product.selected_or_first_available_variant.barcode }}</span>
													</span>
												{% endif %}
											{% endif %}

											{% if block.settings.show_rating %}
												<div class="product-info-meta-item last {% if block.settings.hide_empty_star_rating %}product-rating-hide-empty{% endif %}">
													{% render 'product-rating-widget', product: product %}
												</div>
											{% endif %}
										</div>
									{% endif %}
								</div>
							{% when 'extra' %}
								<div class="product-info-extra" {{ block.shopify_attributes }}>
									{% if block.settings.text != blank %}
										<div class="product-info-extra-aux rte">
											{{ block.settings.text }}
										</div>
									{% endif %}

									{% if block.settings.show_facebook or block.settings.show_twitter or block.settings.show_linkedin %}
										<div class="product-info-extra-sharing">
											<ul class="list-social">
												{% if block.settings.show_facebook %}
													<li class="list-social-item">
														<a href="https://www.facebook.com/sharer.php?u={{ shop.url }}{{ product.url | url_encode }}" target="_blank" class="list-social-link">
															<span class="visually-hidden">{{ 'general.social.share_on_facebook' | t }}</span>
															{% render 'icon-facebook' %}
														</a>
													</li>
												{% endif %}
												{% if block.settings.show_twitter %}
													<li class="list-social-item">
														<a href="https://twitter.com/share?url={{ shop.url }}{{ product.url | url_encode }}" target="_blank" class="list-social-link">
															<span class="visually-hidden">
																{% if settings.social_link_twitter_use_x %}
																	{{ 'general.social.links.x' | t }}
																{% else %}
																	{{ 'general.social.links.twitter' | t }}
																{% endif %}
															</span>
															{% if settings.social_link_twitter_use_x %}
																{% render 'icon-x' %}
															{% else %}
																{% render 'icon-twitter' %}
															{% endif %}
														</a>
													</li>
												{% endif %}
												{% if block.settings.show_linkedin %}
													<li class="list-social-item">
														<a href="https://www.linkedin.com/sharing/share-offsite/?url={{ shop.url }}{{ product.url | url_encode }}" target="_blank" class="list-social-link">
															<span class="visually-hidden">{{ 'general.social.share_on_linkedin' | t }}</span>
															{% render 'icon-linkedin' %}
														</a>
													</li>
												{% endif %}
											</ul>
										</div>
									{% endif %}
								</div>

							{%- when 'price' -%}
                             <div class="price-main-outer" {% if block.settings.price_b_border %}style="border-bottom: 1px solid #000;"{% endif %}>
								{% unless settings.catalog_enabled %}
									<div class="{%- unless product.has_only_default_variant -%}no-js-hidden{%- endunless -%}" id="price-{{ section.id }}" role="status" {{ block.shopify_attributes }}>
										{% render 'price', product: product, discount_badge: true, use_variant: true %}
                                    <div><span>{{ block.settings.price_text }}</span></div>
									</div>

									{%- form 'product', product, id: 'product-form-installment', class: 'installment', data-event-context: 'MainProductPageInstallment' -%}
										<input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}">
										{{ form | payment_terms }}
									{%- endform -%}
								{% endunless %}
                              </div>
							{%- when 'variants' -%}
								{% liquid
									assign swatches_colors = settings.color_swatches | newline_to_br | strip_newlines | split: '<br />'
									assign swatches_options = settings.option_swatches | split: ',' | uniq
									assign words = ''
									assign colors = ''
									for line in swatches_colors
										assign word_color = line | split: ':'
										assign word = word_color[0] | strip
										assign color = word_color[1] | strip
										assign words = words | append: '|' | append: word
										assign colors = colors | append: '|' | append: color
									endfor
									assign words = words | replace_first: '|', '' | split: '|' | compact
									assign colors = colors | replace_first: '|', '' | split: '|' | compact
								%}

								<div class="product-variants variants-hidden {% if product.variants.size <= 1 and block.settings.hide_single_variant %}hidden{% endif %}" {{ block.shopify_attributes }}>
									{%- unless product.has_only_default_variant -%}
										{%- if block.settings.picker_type == 'button' -%}
											<variant-radios
												class="no-js-hidden"
												data-section="{{ section.id }}"
												data-form-id="{{ product_form_id }}"
												data-url="{{ product.url }}"
												data-hide-single="{{ block.settings.hide_single_variant }}"
												data-variants-visibility="{{ block.settings.out_of_stock_variants }}">
												{%- for option in product.options_with_values -%}
													<fieldset class="js product-form-input {% if swatches_options contains option.name %}color-swatch{% endif %}"
															  data-option-name="{{ option.name }}"
													>
														<legend>{{ option.name }}</legend>
														{%- for value in option.values -%}
															<input type="radio" id="{{ section.id }}-{{ option.position }}-{{ forloop.index0 }}"
																name="{{ option.name }}"
																value="{{ value | escape }}"
																form="{{ product_form_id }}"
																{% if option.selected_value == value %}checked{% endif %}
															>
															<label for="{{ section.id }}-{{ option.position }}-{{ forloop.index0 }}">
																{{ value }}
															</label>
														{%- endfor -%}
													</fieldset>
												{%- endfor -%}
												<script type="application/json" data-id="product-options">{{ product.options | json }}</script>
												<script type="application/json" data-id="product-variants">{{ product.variants | json }}</script>
												<script type="application/json" data-id="product-swatches">
													{
														"swatches_on_products": {{ settings.swatches_on_products | json }},
														"swatches_on_filters": {{ settings.swatches_on_filters | json }},
														"swatches_options": {{ swatches_options | json }},
														"swatches": {
															{% for word in words %}
																{% assign color = colors[forloop.index0] %}
																{{ word | json }}: {
																	{% if images[color] != blank %}
																		"color": null,
																		"image": {{ images[color] | image_url: width: 80 | json }}
																	{% else %}
																		"color": {{ color | json }},
																		"image": null
																	{% endif %}
																}{% unless forloop.last %},{% endunless %}
															{% endfor %}
														}
													}
												</script>
											</variant-radios>
										{%- else -%}
											<variant-selects
												class="no-js-hidden"
												data-section="{{ section.id }}"
												data-form-id="{{ product_form_id }}"
												data-url="{{ product.url }}"
												data-hide-single="{{ block.settings.hide_single_variant }}"
												data-variants-visibility="{{ block.settings.out_of_stock_variants }}">
												{%- for option in product.options_with_values -%}
													<div class="product-form-input product-form-input-dropdown">
														<label for="Option-{{ section.id }}-{{ forloop.index0 }}">
															{{ option.name }}
														</label>
														<select
															id="Option-{{ section.id }}-{{ forloop.index0 }}"
															name="options[{{ option.name | escape }}]"
															form="{{ product_form_id }}"
														>
															{%- for value in option.values -%}
																<option value="{{ value | escape }}" {% if option.selected_value == value %}selected="selected"{% endif %}>
																	{{ value }}
																</option>
															{%- endfor -%}
														</select>
													</div>
												{%- endfor -%}

												<script type="application/json" data-id="product-options">{{ product.options | json }}</script>
												<script type="application/json" data-id="product-variants">{{ product.variants | json }}</script>
											</variant-selects>
										{%- endif -%}
									{%- endunless -%}

									<noscript class="product-form-noscript-wrapper-{{ section.id }}">
										<div class="product-form-input{% if product.has_only_default_variant %} hidden{% endif %}">
											<label class="form-label" for="Variants-{{ section.id }}">{{ 'products.product.product_variants' | t }}</label>
											<select name="id" id="Variants-{{ section.id }}" class="select__select" form="{{ product_form_id }}">
												{%- for variant in product.variants -%}
													<option
														{% if variant == product.selected_or_first_available_variant %}selected="selected"{% endif %}
														{% if variant.available == false %}disabled{% endif %}
														value="{{ variant.id }}"
													>
														{{ variant.title }}
														{%- if variant.available == false %} - {{ 'products.product.availability.out_of_stock' | t }}{% endif %}
														- {{ variant.price | money | strip_html }}
													</option>
												{%- endfor -%}
											</select>
										</div>
									</noscript>
								</div>

							{% when 'buy_buttons' %}
								{% unless settings.catalog_enabled %}
									{%- liquid
										assign gift_card_recipient_feature_active = false
										if block.settings.show_gift_card_recipient and product.gift_card?
											assign gift_card_recipient_feature_active = true
										endif

										assign show_dynamic_checkout = false
										if block.settings.show_buy_now and gift_card_recipient_feature_active == false and is_pre_order == false
											assign show_dynamic_checkout = true
										endif
									-%}

									<product-form class="product-form" data-hide-errors="{{ gift_card_recipient_feature_active }}" {{ block.shopify_attributes }}>
										<div class="product-form-error-message-wrapper form-status form-status-error" role="alert" hidden>
											<span class="product-form-error-message"></span>
										</div>

										{% form 'product', product, id: product_form_id, class: 'form', novalidate: 'novalidate', data-type: 'add-to-cart-form', data-stock-threshold: settings.only_x_left, data-event-context: 'MainProductPage' %}
											<input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}" disabled>

											{%- if gift_card_recipient_feature_active -%}
												{%- render 'gift-card-recipient-form', product: product, form: form, section: section -%}
											{%- endif -%}

											<div class="product-actions">
												<div class="product-actions-add-to-cart">
													{% if block.settings.show_quantity %}
														<quantity-input class="quantity-input">
															<button type="button" name="minus" class="quantity-input-button quantity-input-minus no-js-hidden">
																&minus;
																<span class="visually-hidden">{{ 'products.product.quantity.decrease' | t: product: product.title | escape }}</span>
															</button>
															<label for="quantity-input-{{ section.id }}" class="visually-hidden">{{ 'products.product.quantity.label' | t }}</label>
															<input name="quantity"
																type="number"
																class="quantity-input-field"
																id="quantity-input-{{ section.id }}"
																min="1"
																value="1"
																form="{{ product_form_id }}"
															>
															<button type="button" name="plus" class="quantity-input-button quantity-input-plus no-js-hidden">
																&plus;
																<span class="visually-hidden">{{ 'products.product.quantity.increase' | t: product: product.title | escape }}</span>
															</button>
														</quantity-input>
													{% endif %}

													{% if block.settings.show_add_to_cart %}
														<button
															type="submit"
															name="add"
															class="button button-add-to-cart {% if show_dynamic_checkout %} button-secondary{% endif %}"
															{% if product.selected_or_first_available_variant.available == false %} disabled {% endif %}
														>
															{%- if product.selected_or_first_available_variant.available and is_pre_order -%}
																<span>{{ 'products.product.pre_order' | t }}</span>
															{%- elsif product.selected_or_first_available_variant.available -%}
																<span>{{ 'products.product.add_to_cart' | t }}</span>
															{%- else -%}
																<span>{{ 'products.product.availability.out_of_stock' | t }}</span>
															{%- endif -%}

															<div class="button-overlay-spinner hidden">
																<span class="spinner-xs"></span>
															</div>
														</button>
													{% endif %}
												</div>

												{% if show_dynamic_checkout %}
													<div class="product-actions-buy-now">
														{{ form | payment_button }}
													</div>
												{% endif %}
											</div>
										{% endform %}
									</product-form>
								{% endunless %}

							{% when 'availability' %}
								{% unless settings.catalog_enabled %}
									{{ 'component-drawer.css' | asset_url | stylesheet_tag }}
									{{ 'component-pickup-availability.css' | asset_url | stylesheet_tag }}

									{%- assign pick_up_availabilities = product.selected_or_first_available_variant.store_availabilities | where: 'pick_up_enabled', true -%}

									<pickup-availability class="product-pickup-availabilities no-js-hidden"
										{% if product.selected_or_first_available_variant.available and pick_up_availabilities.size > 0 %} available{% endif %}
										data-root-url="{{ routes.root_url }}"
										data-variant-id="{{ product.selected_or_first_available_variant.id }}"
										data-has-only-default-variant="{{ product.has_only_default_variant }}"
										{{ block.shopify_attributes }}
									>
										<template>
											<pickup-availability-preview class="pickup-availability-preview">
												<span class="status-dot status-dot-error"></span>
												<div class="pickup-availability-info">
													<p class="pickup-availability-title">{{ 'products.product.pickup_availability.unavailable' | t }}</p>
													<button class="pickup-availability-button button-text-link">
														{{ 'products.product.pickup_availability.refresh' | t }}
													</button>
												</div>
											</pickup-availability-preview>
										</template>
									</pickup-availability>

									<script src="{{ 'pickup-availability.js' | asset_url }}" defer="defer"></script>
								{% endunless %}

							{%- when 'complementary' -%}
								<script src="{{ 'product-recommendations.js' | asset_url }}" defer="defer"></script>

								<product-recommendations class="product-recommendations" data-url="{{ routes.product_recommendations_url }}?section_id={{ section.id }}&product_id={{ product.id }}&limit={{ block.settings.count }}&intent=complementary">
									{% if recommendations.performed and recommendations.products_count > 0 %}
										<div class="block-block-section-title-wrapper">
											<h2 class="block-section-title">{{ block.settings.title | escape }}</h2>
										</div>

										<div class="product-recommendation-list product-recommendation-list-row">
											{%- for recommendation in recommendations.products -%}
												<div class="product-recommendation-list-row-item">
													{% render 'card-product-media',
														card_product: recommendation,
														show_vendor: block.settings.show_vendor,
														show_stock: false,
														show_rating: block.settings.show_rating,
														show_add_to_cart: block.settings.show_add_to_cart,
														class: 'card-product-media color-background-1'
													%}
												</div>
											{% endfor %}
										</div>
									{% endif %}
								</product-recommendations>

							{%- when 'custom_liquid' -%}
								{% liquid
									if block.settings.position != "right"
										continue
									endif
								%}
								{% capture liquid_output %}{{ block.settings.custom_liquid }}{% endcapture %}
								{% if liquid_output != blank %}
									<div class="custom-liquid-right" {{ block.shopify_attributes }}>
										{% if block.settings.title != blank or block.settings.icon != blank %}
											<h3 class="custom-liquid-right-title">
												{% if block.settings.icon != blank %}
													{% render 'icons', icon: block.settings.icon %}
												{% endif %}
												{{ block.settings.title }}
											</h3>
										{% endif %}

										{{ block.settings.custom_liquid }}
									</div>
								{% endif %}

						{% endcase %}
					{% endfor %}

				</div>
			</div>

			<div class="product-info-description">
				<div class="product-info-description-content">
					{% for block in section.blocks %}
						{% case block.type %}
							{%- when 'custom_liquid' -%}
								{% liquid
									if block.settings.position != "left"
										continue
									endif

									assign is_expandable = false
									if block.settings.title != blank or block.settings.icon != blank
										assign is_expandable = true
									endif
								%}
								{% capture liquid_output %}{{ block.settings.custom_liquid }}{% endcapture %}
								{% if liquid_output != blank %}
									{% if is_expandable %}
										<collapsible-expandable expanded="{{ block.settings.expanded }}" {{ block.shopify_attributes }}>
											<div class="facet">
												<button class="facet-toggle" type="button" aria-expanded="{{ block.settings.expanded }}">
													<span>
														{% if block.settings.icon != blank %}
															{% render 'icons', icon: block.settings.icon %}
														{% endif %}
														{{ block.settings.title }}
													</span>
													{% render 'angle', direction: 'down' %}
												</button>
									{% else %}
										<div class="facet facet-standalone" {{ block.shopify_attributes }}>
									{% endif %}

									<div class="facet-content rte">
										{{ block.settings.custom_liquid }}
									</div>

									{% if is_expandable %}
											</div>
										</collapsible-expandable>
									{% else %}
										</div>
									{% endif %}
								{% endif %}

							{% when 'description' %}
								{% if product.description != blank %}
									<collapsible-expandable expanded="{{ block.settings.expanded }}">
										<div class="facet">
											<button class="facet-toggle" type="button" aria-expanded="{{ block.settings.expanded }}">
												<span class="pp-title">
													{% if block.settings.icon != blank %}
														{% render 'icons', icon: block.settings.icon %}
													{% endif %}
													{{ block.settings.title }}
												</span>
												{% render 'angle', direction: 'down' %}
											</button>

											<div class="pp-details facet-content rte">
												{{ product.description }}
											</div>

										</div>
									</collapsible-expandable>
								{% endif %}

							{% when 'reviews' %}
								<collapsible-expandable expanded="{{ block.settings.expanded }}" id="product-reviews" class="product-reviews">
									<div class="facet">
										<button class="facet-toggle" type="button" aria-expanded="{{ block.settings.expanded }}">
											<span>
												{% render 'icon-star-empty' %}
												{{ 'products.product.reviews' | t }}
											</span>
											{% render 'angle', direction: 'down' %}
										</button>

										<div class="facet-content">
											{% render 'product-review-widget', product: product %}
										</div>
									</div>
								</collapsible-expandable>

							{% when 'expandable' %}
								{% if block.settings.text != blank %}
									<collapsible-expandable expanded="{{ block.settings.expanded }}">
										<div class="facet">
											<button class="facet-toggle" type="button" aria-expanded="{{ block.settings.expanded }}">
												<span>
													{% if block.settings.icon != blank %}
														{% render 'icons', icon: block.settings.icon %}
													{% endif %}
													{{ block.settings.title }}
												</span>
												{% render 'angle', direction: 'down' %}
											</button>

											<div class="facet-content rte">
												{{ block.settings.text }}
											</div>
										</div>
									</collapsible-expandable>
								{% endif %}
						{% endcase %}
					{% endfor %}
				</div>
			</div>

          
		</div>
	</div>
</div>

<script>
	window.variantStock = [];
	{% for variant in product.variants %}
		window.variantStock.push({
			id: {{- variant.id -}},
			quantity: {{- variant.inventory_quantity -}},
			inventory_policy: "{{- variant.inventory_policy -}}",
		});
	{% endfor %}
</script>

{%- if first_3d_model -%}
	<script type="application/json" id="ProductJSON-{{ product.id }}">
		{{ product.media | where: 'media_type', 'model' | json }}
	</script>

	<script src="{{ 'product-model.js' | asset_url }}" defer></script>
{%- endif -%}

{% if product.media.size > 0 %}
	<script src="{{ 'media-gallery.js' | asset_url }}" defer="defer"></script>
	<script src="{{ 'product-modal.js' | asset_url }}" defer="defer"></script>
{% endif %}

{%- liquid
	if product.selected_or_first_available_variant.featured_media
		assign seo_media = product.selected_or_first_available_variant.featured_media
	else
		assign seo_media = product.featured_media
	endif
-%}

<script type="application/ld+json">
	{
		"@context": "http://schema.org/",
		"@type": "Product",
		"name": {{ product.title | json }},
		"url": {{ request.origin | append: product.url | json }},
		{% if seo_media -%}
			"image": [
				{{ seo_media | image_url: width: 1920 | prepend: "https:" | json }}
			],
		{%- endif %}
		"description": {{ product.description | strip_html | json }},
		{% if product.selected_or_first_available_variant.sku != blank -%}
			"sku": {{ product.selected_or_first_available_variant.sku | json }},
		{%- endif %}
		"brand": {
			"@type": "Brand",
			"name": {{ product.vendor | json }}
		},
		"offers": [
			{%- for variant in product.variants -%}
				{
					"@type" : "Offer",
					{%- if variant.sku != blank -%}
						"sku": {{ variant.sku | json }},
					{%- endif -%}
					{%- if variant.barcode.size == 12 -%}
						"gtin12": {{ variant.barcode }},
					{%- endif -%}
					{%- if variant.barcode.size == 13 -%}
						"gtin13": {{ variant.barcode }},
					{%- endif -%}
					{%- if variant.barcode.size == 14 -%}
						"gtin14": {{ variant.barcode }},
					{%- endif -%}
					"availability" : "http://schema.org/{% if variant.available %}InStock{% else %}OutOfStock{% endif %}",
					"price" : {{ variant.price | divided_by: 100.00 | json }},
					"priceCurrency" : {{ cart.currency.iso_code | json }},
					"url" : {{ request.origin | append: variant.url | json }}
				}{% unless forloop.last %},{% endunless %}
			{%- endfor -%}
		]
	}
</script>
