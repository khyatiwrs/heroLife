<link rel="stylesheet" href="{{ 'component-topbar.css' | asset_url }}" media="print" onload="this.media='all'">

<div class="header-top-main">
  <div class="container">
   <div class="header-top">
    <div class="bar-left_side social-icons-main">
      {% if section.settings.show_social %}
        <div class="color-{{ section.settings.color_scheme }}">
            <div class="social-container">
                <div class="footer-social-links-wrap">
                    {% render 'social-media-icons' %}
                </div>
            </div>
        </div>
      {% endif %}
      <div class="social-icons left-text">
        {% if section.settings.text_icon != blank %}
          <span class="media-icon-item-icon">
              {% render 'icons', icon: section.settings.text_icon %}
          </span>
          {% endif %}
          <p>{{ section.settings.left_text }}</p>
      </div>
    </div>
     <div class="bar-center_side">
       <div class="promo-title"> 
         {% if section.settings.shipping_text !=blank %}
         <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="#000000" stroke-width="1.75" stroke-linecap="round" stroke-linejoin="round" class="lucide lucide-truck">
           <path d="M14 18V6a2 2 0 0 0-2-2H4a2 2 0 0 0-2 2v11a1 1 0 0 0 1 1h2"></path><path d="M15 18H9"></path><path d="M19 18h2a1 1 0 0 0 1-1v-3.65a1 1 0 0 0-.22-.624l-3.48-4.35A1 1 0 0 0 17.52 8H14"></path><circle cx="17" cy="18" r="2"></circle>
           <circle cx="7" cy="18" r="2"></circle></svg> 
         <span>{{ section.settings.shipping_text }}</span>
         {% endif %}
       </div>
     </div>
    <div class="bar-right_side">
      <div class="bar-right-content">
        {% if section.settings.link1_url != blank and section.settings.link1_text != blank %}
        <a class="tb-link link1" href="{{ section.settings.link1_url }}"> {% render 'icons', icon: section.settings.link1_icon %}
        <span class="link-title">{{ section.settings.link1_text }}</span></a>
          {%- endif -%}
        
          <!-- distributer -->
        {% if section.settings.link2_url != blank and section.settings.link2_text != blank %}
          {%- unless customer -%}
        <a class="tb-link link2" href="{{ section.settings.link2_url }}"> {% render 'icons', icon: section.settings.link2_icon %}
        <span class="link-title"></span>{{ section.settings.link2_text }}</a>
            {% endunless %}
          {%- endif -%}
      

          <!-- register -->
          {% if section.settings.link3_url != blank and section.settings.link3_text != blank %}
            {%- unless customer -%}
        <a class="tb-link link3" href="{{ section.settings.link3_url }}"> {% render 'icons', icon: section.settings.link3_icon %}
        <span class="link-title">{{ section.settings.link3_text }}</span></a>
            {% endunless %}
          {%- endif -%}
        <!-- wishlist -->
          {% if section.settings.en_wishlist %}
            {%- if customer -%}
             <a class="tb-link link3" href="{%- if customer -%}/wishlist{%- endif -%}">
                <span class="visually-hidden">
                    {%- if customer -%}
                        {{ 'customer.account_fallback' | t }}
                    {%- else -%}
                        {{ 'customer.log_in' | t }}
                    {%- endif -%}
                </span>
             {% render 'icon-heart' %}
              Wishlist</a>
              {%- endif -%}
          {% endif %}
          
         <!-- signin  -->
        {% if section.settings.en_signin %}
          <a class="tb-link link4" href="{%- if customer -%}{{ routes.account_url }}{%- else -%}{{ routes.account_login_url }}{%- endif -%}">
          <span class="visually-hidden">
              {%- if customer -%}
                  {{ 'customer.account_fallback' | t }}
              {%- else -%}
                  {{ 'customer.log_in' | t }}
              {%- endif -%}
          </span>
          {% render 'icon-user' %}
          Â {%- if customer -%}Hi, {{ customer.first_name }} 
             <a href="{{ routes.account_logout_url }}" class="tb-link link4">{% render 'icon-log-out' %}{{ 'customer.log_out' | t }}</a>
             {%- else -%}Sign In{%- endif -%}</a>
          {% endif %}
      </div>
    </div>
      </div>
  </div>
</div>
<style>
  .header-top-main{
    background:{{ section.settings.bar_bg }};
    color:{{ section.settings.bar_color }};
  }
  .header-top{
     .tb-link,.promo-title span,.promo-title p{
    color:{{ section.settings.bar_color }};
   }
  }
  .header-top {
  .media-icon-item-icon svg path, p, .list-social-link svg path,.link1 svg path,.link2 svg path,.link3 svg path,.link4 svg path,.promo-title svg
    {
      fill:{{ section.settings.bar_color }};
    }
  }
  .promo-title{
  display: flex;
    align-items: center;
    padding: 5px;
  }
</style>
